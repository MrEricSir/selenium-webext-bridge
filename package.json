{
  "name": "selenium-webext-bridge",
  "version": "0.2.0",
  "description": "Selenium/Firefox WebExtension test bridge - control and query any Firefox extension from Selenium tests",
  "main": "index.js",
  "scripts": {
    "test": "node tests/bridge-api.test.js",
    "test:coverage": "c8 --reporter=text --reporter=lcov --include='lib/**' --include='index.js' node tests/bridge-api.test.js"
  },
  "keywords": [
    "selenium",
    "firefox",
    "webextension",
    "testing",
    "browser-extension",
    "integration-testing",
    "webdriver"
  ],
  "author": "Eric Muyser",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MrEricSir/selenium-webext-bridge.git"
  },
  "files": [
    "index.js",
    "lib/",
    "extension/",
    "README.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "geckodriver": "*",
    "selenium-webdriver": "^4.0.0"
  },
  "devDependencies": {
    "c8": "^10.1.3"
  }
}
